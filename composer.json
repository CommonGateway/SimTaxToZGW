{
    "name": "common-gateway/sim-tax-to-zgw-bundle",
    "description": "Deze bundle bevat de benodigde logica (schema's, mappings en code) voor het omzeten van Sim Form belastingen xml naar ZGW berichten. Ten behoeve van het inrichten van belasting aangiften en bezwaren.",
    "type": "symfony-bundle",
    "keywords": [
        "commongateway",
        "common",
        "gateway",
        "conduction",
        "symfony",
        "common-gateway-plugin",
        "sim",
        "tax",
        "zgw"
    ],
    "homepage": "https://commongateway.nl",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "commongateway/corebundle": "^1.0.67",
        "common-gateway/open-belasting-bundle": "^0.0.8"
    },
    "require-dev": {
        "symfony/dependency-injection": "~3.4|~4.1|~5.0"
    },
    "autoload": {
        "psr-4": {
            "CommonGateway\\SimTaxToZGWBundle\\": "src/"
        }
    }
}
