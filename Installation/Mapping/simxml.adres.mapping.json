{
  "title": "Adres",
  "$id": "https://dowr.simxml.nl/mapping/simxml.adres.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.1",
  "passTrough": false,
  "mapping": {
    "@soortEntiteit": "F",
    "postcode": "{{ postcode }}",
    "woonplaatsnaam": "{{ woonplaatsnaam }}",
    "straatnaam": "{{ straatnaam }}",
    "huisnummer": "{{ huisnummer }}",
    "huisletter": "{{ huisletter }}",
    "huisnummertoevoeging": "{{ huisnummertoevoeging }}",
    "aanduidingBijHuisnummer.@xsi:nil": true,
    "aanduidingBijHuisnummer.@StUF:noValue": "geenWaarde",
    "locatieomschrijving": "{{ locatieomschrijving }}"
  }
}
