{
  "title": "Adres",
  "$id": "https://dowr.simxml.nl/mapping/simxml.adres.mapping.json",
  "$schema": "https://docs.commongateway.nl/schemas/Mapping.schema.json",
  "version": "0.0.1",
  "passTrough": false,
  "mapping": {
    "@soortEntiteit": "F",
    "postcode": "{{ value.postcode }}",
    "woonplaatsnaam": "{{ value.woonplaatsnaam }}",
    "straatnaam": "{{ value.straatnaam }}",
    "huisnummer": "{{ value.huisnummer }}",
    "huisletter": "{{ value.huisletter }}",
    "huisnummertoevoeging": "{{ value.huisnummertoevoeging }}",
    "aanduidingBijHuisnummer.@xsi:nil": "true",
    "aanduidingBijHuisnummer.@StUF:noValue": "geenWaarde",
    "locatieomschrijving": "{{ value.locatieomschrijving }}"
  }
}
